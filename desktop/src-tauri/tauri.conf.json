{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "QuantumLiquidity",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.quantumliquidity.desktop",
    "icon": []        },
  },
    "csp": null,
    "windows": [      {
        "fullscreen": false,
        "resizable": true,
        "title": "QuantumLiquidity",
        "width": 1600,
        "height": 1000,
        "minWidth": 1200,
        "minHeight": 800
      }
    ]
  }
}
